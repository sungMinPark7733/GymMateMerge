<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".GymmateApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gymmate"
        tools:targetApi="31">
        <activity
            android:name=".ui.questionpage.Email"
            android:exported="false" />
        <activity
            android:name=".ui.questionpage.Signin"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.questionpage.Age"
            android:exported="true" />
        <activity
            android:name=".ui.questionpage.Calories"
            android:exported="true" />
        <activity
            android:name=".ui.questionpage.Confirmation"
            android:exported="true" />
        <activity
            android:name=".ui.questionpage.Days"
            android:exported="true" />
        <activity
            android:name=".ui.questionpage.Gender"
            android:exported="true" />
        <activity
            android:name=".ui.questionpage.Goal"
            android:exported="true" />
        <activity
            android:name=".ui.questionpage.Height"
            android:exported="true" />
        <activity
            android:name=".ui.questionpage.Name"
            android:exported="true" />
        <activity
            android:name=".ui.questionpage.Weight"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />

    </application>


</manifest>